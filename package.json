  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "tslib": "^2.4.0",
    "tsup": "^6.7.0"
  }
}
